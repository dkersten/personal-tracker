nst state = useSelector(modalState)\n\n    const [modalIsOpen, setModalIsOpen] = useState(false)\n    const [dayActivities, setDayActivities] = useState(null)\n\n    useEffect(() => {\n        if (state.modalType !== null) {\n            setModalIsOpen(true)\n\n            if (state.modalType === 'monthView') {\n                fetch(`http://localhost:3001/activities/date/${state.modalProps}`)\n                    .then(resp => resp.json())\n                    .then(activities => setDayActivities(activities))\n            }\n\n        } else if (state.modalType === null) {\n            setModalIsOpen(false)\n        }\n    }, [state.modalType])\n\n    const dispatch = useDispatch()\n\n    const renderHeader = () => {\n        if (state.modalType === 'monthView') {\n\n            return (<span>Activities for: {state.modalProps}</span>)\n        } else if (state.modalType === 'yearView') {\n            return ('Year View')\n        } else {\n            return null\n        }\n    }\n\n    const renderMonthView = () => {\n        const activitiesArr = []\n\n        if (dayActivities !== null ) {\n            for (let i = 0; i < dayActivities.length; i++) {\n                activitiesArr.push(<MonthModalActivity \n                        key={i}\n                        category={dayActivities[i].category}\n                        name={dayActivities[i].name}\n                        description={dayActivities[i].description}\n                    /> )\n            }\n\n            return activitiesArr\n        } else {\n            return \"please wait\"\n        }\n        \n    }\n\n    const closeModalEventHandler = () => {\n        dispatch(closeModal())\n    }\n\n    return(\n        <GeneralModal\n            show={modalIsOpen}\n            aria-labelledby=\"contained-modal-title-vcenter\"\n            centered\n        >\n            <Modal.Header>\n                <h3>{renderHeader()}</h3>\n            </Modal.Header>\n            <Modal.Body>\n                { state.modalType === \"monthView\" ? renderMonthView() : \"no activities\"}\n                { state.modalType === \"yearView\" ? 'year view' : null}\n            </Modal.Body>\n            <Modal.Footer>\n                <button onClick={closeModalEventHandler}>Close</button>\n            </Modal.Footer>\n        </GeneralModal>\n    )\n}\n\nexport default ReusableModal","/Users/dkersten/Development/code/personal-tracker/src/components/DatePicker.js",[],"/Users/dkersten/Development/code/personal-tracker/src/reducers/index.js",[],"/Users/dkersten/Development/code/personal-tracker/src/reducers/activitiesReducer.js",[],"/Users/dkersten/Development/code/personal-tracker/src/actions/activityActions.js",[],"/Users/dkersten/Development/code/personal-tracker/src/reducers/activitiesMonthReducer.js",[],"/Users/dkersten/Development/code/personal-tracker/src/actions/activityMonthActions.js",[],"/Users/dkersten/Development/code/personal-tracker/src/reducers/modalReducer.js",[],"/Users/dkersten/Development/code/personal-tracker/src/actions/modalActions.js",[],"/Users/dkersten/Development/code/personal-tracker/src/stateSelectors.js",[],"/Users/dkersten/Development/code/personal-tracker/src/components/MonthModalActivity.js",[],"/Users/dkersten/Development/code/personal-tracker/src/renderActivityIcons.js",[],["151","152"],"/Users/dkersten/Development/code/personal-tracker/src/containers/PlanningContainer.js",[],{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","replacedBy":"156"},{"ruleId":"153","replacedBy":"157"},{"ruleId":"155","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":79,"column":8,"nodeType":"161","endLine":79,"endColumn":25,"suggestions":"162"},{"ruleId":"153","replacedBy":"163"},{"ruleId":"155","replacedBy":"164"},"no-native-reassign",["165"],"no-negated-in-lhs",["166"],["165"],["166"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'state.modalProps'. Either include it or remove the dependency array.","ArrayExp